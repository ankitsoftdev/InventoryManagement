@model ViewModel.Transactions.DeliveryNoteMasterInfo
<div class="panel">

    <div class="panel-heading bg-aqua">
        <span>View Details</span>
    </div>

    <div class="panel-body" style="border:silver 1px;">

        <table class="table table-bordered table-striped">
            <tr><td colspan="2"><label>Customer Details</label></td></tr>
            <tr><td>Code</td><td><span id="Customer_Code">@Html.DisplayFor(x => x.Customer_Code)</span></td></tr>
            <tr><td>Name</td><td><span id="Name"> @Html.DisplayFor(x => x.Name)</span></td></tr>
            <tr><td>Contact No</td><td><span id="Contact_No"> @Html.DisplayFor(x => x.Contact_No)</span></td></tr>
            <tr><td>Email</td><td><span id="Email"> @Html.DisplayFor(x => x.Email)</span></td></tr>
            <tr><td>Address</td><td><span id="Address"> @Html.DisplayFor(x => x.Address)</span></td></tr>
            <tr><td>Order Date</td><td><span id="Order_Date"> @Html.DisplayFor(x => x.Order_Date)</span></td></tr>


        </table>

