
@model List<ViewModel.CommonReport .Financial_Year >


<table class="table table-striped table-bordered table-hover table-responsive topmrgn5px">
    <tr>
        <td>
            Name
        </td>
        <td>
            From Period
        </td>

        <td>
            To Period
        </td>


    </tr>
    @if (Model.Count != 0)
    {
        foreach (var item in Model)
        {
            
            <tr>
                <td>
                    @Html.DisplayFor(x => item.Name)

                </td>
                <td>
                    @Html.DisplayFor(x => item.FromPeriod)
                </td>

                <td>
                    @Html.DisplayFor(x => item.ToPeriod)
                </td>


            </tr>
        }

    }
    else
    {
        <tr>
            <td colspan="4">
                No Records found..
            </td>
        </tr>
    }


</table>